{
	"name": "@rbxts/ejt",
	"version": "2.0.0",
	"description": "A library for interacting with difficulties in the EToH Joke Towers wiki.",
	"main": "out/init.lua",
	"scripts": {
		"build": "rbxtsc",
		"watch": "rbxtsc -w",
		"asset-sync": "npx rbxtsas --path=difficulties/image --output=difficulties/assetMap.ts",
		"generate:difficulties": "node scripts/generate-difficulties.js",
		"generate:index": "node scripts/generate-index-difficulties.js",
		"pub": "npm run build & npm publish & wally publish"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/evilbocchi/jjt-difficulties.git"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"types": "out/index.d.ts",
	"files": [
		"out",
		"!**/*.tsbuildinfo"
	],
	"publishConfig": {
		"access": "public"
	},
	"devDependencies": {
		"@rbxts/compiler-types": "^3.0.0-types.0",
		"@rbxts/types": "^1.0.879",
		"@typescript-eslint/eslint-plugin": "^8.41.0",
		"@typescript-eslint/parser": "^8.41.0",
		"eslint": "^9.34.0",
		"eslint-plugin-roblox-ts": "^1.1.0",
		"pngjs": "^6.0.0",
		"rbx-lune": "^1.0.0",
		"rbxts-asset-sync": "^0.3.2",
		"roblox-ts": "^3.0.0",
		"typescript": "^5.5.3",
		"unidecode": "^0.1.8"
	}
}
